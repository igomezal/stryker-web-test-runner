    "@esm-bundle/chai": "^4.1.5",
    "@open-wc/testing": "^2.5.32",
    "@stryker-mutator/core": "^4.3.1",
    "@stryker-mutator/mocha-runner": "^4.3.1",
    "@web/test-runner": "^0.12.5"
  },
  "dependencies": {
    "lit-element": "^2.4.0"
  }
}
